import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        String s=scan.nextLine();
        int n=s.length();
        int [] freq=new int[256];
        for(int i=0;i<n;i++){
        freq[s.charAt(i)]++;
          if(freq[s.charAt(i)]>1){
            System.out.println("No");
            return;
          }
        }
        System.out.println("Yes");        
    }
}
    
