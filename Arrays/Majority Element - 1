import java.io.*;
import java.util.*;


        class Solution {
    public int majorityElement(int[] nums) {
      int n=nums.length;
      
      int mc=n/2;
      for(int i=0;i<n;i++){
        int c=0;
        for(int j=0;j<n;j++){
            if(nums[i]==nums[j])
            c++;
        }
      
    
        if(c>mc)
       return nums[i];
      }
        
      return -1;
    }
      }
        
  
