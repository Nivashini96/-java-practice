import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        String d=scan.nextLine();
        char n=scan.next().charAt(0);
        int c=0;
        for(int i=0;i<d.length();i++){
            if(n==d.charAt(i)){
                c++;
            }
        }
        System.out.println(c);
        
       
    }
}
