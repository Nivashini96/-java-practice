import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         import java.util.*;
 class Solution {
    public boolean isSorted(int[] arr) {
        int n=arr.length;
        for(int i=0;i<n-1;i++){
            if(arr[i]<arr[i+1]){
                return true;
            }
        }
        return false;
    }
 }
