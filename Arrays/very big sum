import java.io.*;
class Solution {


    public static void main(String[] args)
    {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        long a[]=new long[n];
        long sum=0;
        for(int i=0;i<n;i++)
        {
        
            a[i]=scan.nextLong();
        }
            for(int i=0;i<n;i++)
            {
              sum+=a[i];
        }
        System.out.print(sum);
        

    }

}

