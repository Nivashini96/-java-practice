import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        int nums[]=new int[n];
      int a=0;
      int b=0;
      for(int i=0;i<=n;i++){
        a+=i;
        }
        for(int i=0;i<n;i++){
            b+=nums[i];
        }
        int miss=a-b;
        
        }
}
