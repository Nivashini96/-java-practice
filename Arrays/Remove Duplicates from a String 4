import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        String t = "";
       for(int i=0;i<s.length();i++){
           char c = s.charAt(i);
           if(t.indexOf(c) == -1)
               t+=c;
       }
        System.out.println(t);
        
    }
}
