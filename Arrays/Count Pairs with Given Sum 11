import java.io.*;
import java.util.*;


    int countPairs(int arr[], int target) {
       
        int n=arr.length;
        int c=0;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++)
            {
                if(arr[i]+arr[j]==target)
                c++;
                
        }
        
        }
        
    return c;
    }
}
  
