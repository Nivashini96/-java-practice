import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        int r=0;
        int t=n;
        
        while(n>0){
            r=(n%10)+(r*10);
            n=n/10;
        }
        if(r==t){
        System.out.print("true");}
        else{
            System.out.print("false");}
    }     
}
